{
  "expo": {
    "name": "myPets",
    "slug": "mobile-pet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/logoPet.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "mobilepet",
    "splash": {
      "image": "./assets/img/logoPet.png",
      "resizeMode": "contain",
      "backgroundColor": "#332630"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Este app precisa acessar sua galeria de fotos para você selecionar uma foto de perfil.",
        "NSCameraUsageDescription": "Este app precisa acessar sua câmera para você tirar uma foto de perfil."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/logoPet.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.lucas552.mobilepet",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "mobilepet"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-sqlite"],
    "extra": {
      "eas": {
        "projectId": "6e73d393-3951-4eb2-8682-b0977c705a0a"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    }
  }
}
